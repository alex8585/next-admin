{
  "name": "next-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "eslint ./{components,hooks,layout,pages}/**/*.{js,ts,tsx}",
    "lint:fix": "eslint --fix ./{components,hooks,layout,pages}/**/*.{js,ts,tsx}",
    "typecheck": "tsc --noEmit  --project . --composite false",
    "pre_commit_script": "npm run typecheck && npm run lint:fix",
    "_old_prettier": "npx prettier  --check ./{components,hooks,layout,pages}/**/*.{js,ts,tsx}",
    "_old_prettier:fix": "npx prettier  --write ./{components,hooks,layout,pages}/**/*.{js,ts,tsx}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "bash -c tsc -p tsconfig.json --noEmit"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.3",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/x-data-grid": "^5.12.1",
    "@types/axios": "^0.14.0",
    "@types/cookie": "^0.5.1",
    "axios": "^0.27.2",
    "cookie": "^0.5.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^13.0.3",
    "next": "12.1.6",
    "query-string": "^7.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.8",
    "typescript": "4.7.3"
  }
}
